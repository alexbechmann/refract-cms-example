{
  "name": "razzle-examples-with-typescript",
  "version": "2.4.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p docker:start razzle:start",
    "razzle:start": "razzle start",
    "docker:start": "docker-compose up",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "heroku-postbuild": "node --max-old-space-size=1000 ./node_modules/razzle/bin/razzle.js build"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@refract-cms/core": "0.5.2-alpha.299",
    "@refract-cms/dashboard": "0.5.2-alpha.299",
    "@refract-cms/server": "0.5.2-alpha.299",
    "@types/googlemaps": "^3.30.16",
    "@types/react-jss": "^8.6.0",
    "apollo-client": "^2.4.8",
    "autolinks": "^0.1.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "combine-containers": "^0.1.3-alpha",
    "express": "^4.16.4",
    "graphql-request": "^1.8.2",
    "isomorphic-fetch": "^2.2.1",
    "logrocket": "^0.6.19",
    "logrocket-react": "^3.0.0",
    "material-ui-image": "^3.0.3",
    "mini-css-extract-plugin": "^0.4.5",
    "moment": "^2.22.2",
    "nodemailer": "^4.7.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "razzle-heroku": "^3.0.0",
    "react": "16.8.6",
    "react-apollo": "^2.5.3",
    "react-dom": "16.8.6",
    "react-google-maps": "^9.4.5",
    "react-hot-loader": "^4.6.3",
    "react-jss": "^10.0.0-alpha.6",
    "react-redux": "5.1.1",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-promise": "^0.6.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/node": "9.6.6",
    "@types/nodemailer": "^4.6.5",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.2.6",
    "@types/recompose": "^0.30.5",
    "@types/redux-form": "^7.4.14",
    "@types/redux-promise": "^0.5.28",
    "@types/webpack-env": "^1.13.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "prettier-loader": "^2.1.1",
    "razzle": "^2.4.0",
    "razzle-plugin-typescript": "^2.4.0",
    "ts-jest": "^22.4.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack-babel-env-deps": "^1.4.3"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
